<%- include("partials/header"); -%>
<h1>Home</h1>

<p><%= homeStartingContent %></p>

 <% posts.forEach(function(post) { %>
  <h3> <%= post.title %> </h3>
  <% var content = post.content.substring(0,100); %>
  <p>
    <%= content + " ...." %>
    <a href="/posts/<%= post.title %>">Read More</a>
  </p>
 <% }) %>

<%- include("partials/footer"); -%>
